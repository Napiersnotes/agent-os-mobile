[run]
source = src
omit = 
    src/__pycache__/*
    src/*/__pycache__/*
    tests/*
    */migrations/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*Test
    @property
    @abstractmethod

[html]
directory = coverage_html
